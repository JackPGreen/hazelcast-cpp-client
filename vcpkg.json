{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "hazelcast-cpp-client",
  "version": "5.5.0",
  "builtin-baseline": "50c0cb48a0cf2f6fc5c7b2c0d2bafbe26d0a7ca2",
  "dependencies": [
    {
      "name": "boost",
      "version>=": "1.88.0",
      "features": [
        "thread",
        "asio",
        "chrono",
        "format",
        "optional",
        "property-tree",
        "system",
        "uuid",
        "multiprecision"
      ]
    },
    "openssl"
  ],
  "features": {
    "build-tests": {
      "description": "Enable GTest for building tests",
      "dependencies": [
        "gtest",
        "thrift"
      ]
    }
  }
}
